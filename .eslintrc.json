{
  "extends": "@nodutilus",
  "overrides": [
    {
      "files": [
        "./test.js"
      ],
      "env": {
        "browser": false,
        "node": true
      }
    }
  ],
  "rules": {
    "jsdoc/require-jsdoc": 0,
